<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui' 
  xmlns:dt='urn:import:com.aridhu.gwt.pricing.client.widgets'>
  <ui:style>
    .all {
    	float: left;
    }
    
    .cb {
    	display: block;
    }
    
    .none {
    	margin-left: 5px;
    }
  </ui:style>
  <g:FlowPanel>
  
    <g:Button ui:field="all" styleName="{style.all}">Manage Items</g:Button>
    <g:Button ui:field="none" styleName="{style.none}">Manage Price Lists</g:Button>
  </g:FlowPanel>
</ui:UiBinder>